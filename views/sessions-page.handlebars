{{>header}}
<main>
<div class="info">
	<h2>Stay organized.</h2>
	<h3>To log in select a session below:</h3>
		<div class="select-container">
			<select id="session-select">
			<option value="" disabled selected>Select a session</option>
			{{#each session}}
				{{#if name}}
					<option value={{this.idi}}>{{this.name}}</option>
				{{/if}}
			{{/each}}
			</select>
		</div>
</div>
</main>
{{>footer}}

{{>add-session}}